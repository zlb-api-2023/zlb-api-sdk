{
    "name": "zlb-v3/zlb-api-sdk",
    "description": "zlb-sdk",
    "keywords": [
        "zlb"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "xigua",
            "email": "zhao0yan@qq.com"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Zlb\\SDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { "Zlb\\Tests\\": "tests/" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
